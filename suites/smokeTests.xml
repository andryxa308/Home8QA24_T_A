<?xml version="1.0" encoding= "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SmokeTest" >
    <test name="By groups">
        <parameter name="browserName" value="chrome"/>
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="tests">
            </package>
        </packages>
    </test>
</suite>